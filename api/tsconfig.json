{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "strict": true,
    "typeRoots": [
      "./",
      "../node_modules/@types"
    ],
    "types": [
      "env",
      "@types/node",
      "vite/client"
    ],
    "baseUrl": "src",
    "paths": {
      "~/*": ["./*"]
    }
  },
  "include": [
    "src",
    "vite.config.ts"
  ],
  "exclude": [
  ]
}
