datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

// Define your own datamodels here and run `yarn redwood prisma migrate dev`

model CouncilEvent {
  order      BigInt   @id @default(autoincrement())
  created_at DateTime
  stream_id  String
  sequence   BigInt
  data       Json
  is_deleted Boolean
  tags       String[]

  @@unique([stream_id, sequence])
}
