{
  "name": "council",
  "sources": [
    {
      "dir" : "src",
      "subdirs" : true
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "suffix": ".bs.js",
  "gentypeconfig": {
    "language": "typescript",
    "shims": {
      "Js": "Js"
    },
    "generatedFileExtension": ".gen.ts",
    "module": "es6"
  },
  "bs-dependencies": [
    "@rescript/react"
  ]
}
